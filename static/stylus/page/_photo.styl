.photo_bg
	background-image:  url(../img/pattern1.png), url(../img/bg_photo.jpg)
	width 100%
	height 100vh
	background-size 2px, cover
	opacity 0.83
	background-attachment fixed, fixed
.photo
	h3
		font-weight 400
		font-size 28px
	.galery
		border 1px solid greyl
		padding 30px
		display -webkit-flex
		display -moz-flex
		display -ms-flex
		display -o-flex
		display flex
		flex-direction row
		flex-wrap wrap
		justify-content center
		width 95%
		margin-top 20px
		img
			width 102px
			height 77px
			object-fit cover
			margin 0 0 25px 25px