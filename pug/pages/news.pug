extends ../layout/main

block title
	|Новости студии   Школа свадебного танца Ты со мной
block bgbody
	.news_bg.site_head
		.site_title
			span -в ритме любви-
			h1 Студия свадебного танца
		a(href="#").scrollb
			img.scroll(src="static/img/mo.png", alt="alt")
block content
